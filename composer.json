{
	"name" : "n2n/n2n-l10n",
	"type" : "library",
	"description" : "n2n l10n support",
	"keywords" : [
		"n2n",
		"l10n"
	],
	"homepage" : "https://n2n.rocks/",
	"license" : "LGPL-3.0-or-later",
	"minimum-stability" : "dev",
	"prefer-stable": true,
	"config" : {
		"vendor-dir" : "src/vendor"
	},
	"authors" : [{
		"name" : "Andreas von Burg",
		"email" : "a@von-burg.net",
		"homepage" : "https://www.von-burg.net/"
	}
	],	
	"minimum-stability" : "dev",
	"prefer-stable": true,
	"config" : {
